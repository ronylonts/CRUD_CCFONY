{
    "name": "votre-projet/crud-ccfony",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "symfony/security-bundle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/games-package/games-package"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}